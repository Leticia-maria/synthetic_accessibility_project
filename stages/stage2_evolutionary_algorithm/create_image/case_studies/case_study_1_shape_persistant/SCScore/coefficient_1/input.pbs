#!/bin/sh

#PBS -j oe
#PBS -lwalltime=48:00:00
#PBS -lselect=1:ncpus=32:mem=62gb

module load anaconda3/personal
source activate py37

cd $PBS_O_WORKDIR

python -m stk.ea $(realpath $PBS_O_WORKDIR/input.py) &> ea_run.log
NUM_EA_RUNS=$(ls stk_ea_runs | wc -l)
NUM_EA_RUNS=$NUM_EA_RUN-1
mv ea_run.log stk_ea_runs/"$NUM_EA_RUNS"